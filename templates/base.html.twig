<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <img src="{{ asset('build/images/symfony-logo.svg') }}" alt="Symfony"><br>
            <img src="{{ source('symfony-logo.svg')|data_uri }}" alt="Symfony inlined 1"><br>
            <img src="{{ source('images/symfony-logo.svg')|data_uri }}" alt="Symfony inlined 2"><br>
            <img src="{{ source('build/images/symfony-logo.svg')|data_uri }}" alt="Symfony inlined 3"><br>

            <img src="{{ source('symfony-logo.37a94574.svg')|data_uri }}" alt="Symfony inlined 4"><br>
            <img src="{{ source('images/symfony-logo.37a94574.svg')|data_uri }}" alt="Symfony inlined 5"><br>
            <img src="{{ source('build/images/symfony-logo.37a94574.svg')|data_uri }}" alt="Symfony inlined 6"><br>

            <img src="{{ source(asset('build/images/symfony-logo.svg'))|data_uri }}" alt="Symfony inlined 7">
        {% endblock %}
    </body>
</html>
